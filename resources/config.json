          "primarykey": true,
          "type": "integer",
          "value": {
            "type": "autoincrement"
          }
        },
        "salesperson": {
          "type": "association",
          "select": {
            "model": "salesperson",
            "field": "id",
            "filter": "$model.created_at < $self.created_at",
            "weight": "skill"
          },
          "value": {
            "model": "salesperson",
            "filter": "$model.created_at < $self.created_at",
            "weight": "skill"
