          "value": {
            "type": "formula",
            "properties": [{
              "equation": "2 + 3 * x",
              "randomness": 0.3
            }, {
              "start": 5,
              "randomness": 0.6
            }]
          }
        },
        "updated_at": {
          "type": "datetime",
          "value": {
            "type": "case",
            "check": "$self.active",
            "branches": {
              "true": {
                "type": "distribution",
                "properties": {
                  "mean": "$self.created_at + 30000",
                  "std": 15000,
                  "constraint": {
                    "min": "$self.created_at + 100000",
                    "offset": 100000
